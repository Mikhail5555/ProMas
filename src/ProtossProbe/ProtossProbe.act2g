use ProtossProbe as knowledge.
use "../General/General" as knowledge.

% Only execute if the field is not already gathered.
define gather(Id) with
	pre { not(gathered(Id)) }
	post { true }
	
% Builds a building.
define build(Id, X, Y) with
	pre { true }
	post { true }
	
% Move to an location.
define move(X, Y) with
	pre { true }
	post { true }
