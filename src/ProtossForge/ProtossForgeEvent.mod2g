use ProtossForge as knowledge.
use "../General/General" as knowledge.
use "../General/GeneralEvents" as module.

module ProtossForgeEvent {
	
	% Run the general module
	if true
		then GeneralEvents.
	
	% Update the upgrading percept
	forall percept(upgrading(Upgrade)), not(bel(upgrading(Upgrade))) 
		do insert(upgrading(Upgrade)).
	forall bel(upgrading(Upgrade)), not(percept(upgrade(Upgrading))) 
		do delete(upgrading(Upgrade)).
	
}